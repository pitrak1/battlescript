class bool(int):
    def __init__(self, value):
        if value:
            self.__btl_value = __btl_numeric__(1)
        else:
            self.__btl_value = __btl_numeric__(0)

    def __str__(self):
        if self.__btl_value:
            return "True"
        else:
            return "False"

    def __repr__(self):
        if self.__btl_value:
            return "True"
        else:
            return "False"
